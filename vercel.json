
{
  "version": 2
  "builds": [
    {
      "src": "deploy-heroku.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/deploy-heroku.js"
    },
    {
      "source": "/(.*)",
      "destination": "/deploy-heroku.js"
    }
  ]
}

